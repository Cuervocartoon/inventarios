{
	"name": "danielromeroauk/inventarios",
	"description": "Sistema de inventarios.",
	"keywords": ["framework", "laravel", "inventarios", "adsiar", "construimportados"],
	"require": {
		"laravel/framework": "4.0.*",
		"way/generators": "dev-master",
		"twitter/bootstrap": "v2.3.2"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize",
			"php artisan asset:publish bootstrap --path=vendor/twitter/bootstrap/docs/assets/"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
